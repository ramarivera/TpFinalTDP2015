<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Cuestionario.Model"
                   namespace="Cuestionario.Model">

  <class name="AnswerSession" table="AnswerSessions">
    <id name="Id" column="id">
      <generator class="identity"/>
    </id>
    <property name="Username" column="username" />
    <property name="CategoryId" column="categoryId" />
    <property name="DifficultyId" column="difficultyId" />
    <property name="AnswerTime" column="answerTime" />
    <property name="Score" column="score" />
    <property name="Date" column="date" />
  </class>

</hibernate-mapping>
