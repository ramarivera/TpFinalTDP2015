<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Cuestionario.Model"
                   namespace="Cuestionario.Model">

  <class name="UserAnswer" table="UserAnswers">
    <id name="Id" column="id">
      <generator class="identity"/>
    </id>
    <property name="QuestionId" column="questionId" />
    <property name="AnswerSessionId" column="answerSessionId" />
    <property name="ChosenAnswerId" column="chosenAnswerId" />
    <property name="AnswerStatus" column="answerStatus" />
  </class>

</hibernate-mapping>