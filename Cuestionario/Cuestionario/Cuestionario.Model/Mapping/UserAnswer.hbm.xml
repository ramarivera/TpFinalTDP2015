<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Cuestionario.Model"
                   namespace="Cuestionario.Model">

  <class name="UserAnswer" table="UserAnswers">
    <id name="pId" column="id">
      <generator class="identity"/>
    </id>
    <property name="pQuestionId" column="questionId" />
    <property name="pAnswerSessionId" column="answerSessionId" />
    <property name="pChosenAnswerId" column="chosenAnswerId" />
    <property name="pAnswerStatus" column="answerStatus" />
  </class>

</hibernate-mapping>